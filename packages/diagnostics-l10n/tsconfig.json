{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist",
    "rootDir": "src",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUncheckedIndexedAccess": false,
    "noUnusedParameters": false,
    "module": "esnext",
    "target": "esnext"
  },
  "include": ["src/**/*.ts", "src/locales/*.json"]
}
